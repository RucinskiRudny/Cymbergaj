<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="pl.javastart.AirHockeyActivity"
    tools:ignore="MergeRootFrame" >

    <TextView
        android:id="@+id/NazwaX1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="40dp"
        android:text="@string/NazwaX"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/NazwaX2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="100dp"
        android:text="@string/NazwaY"
        android:textAppearance="?android:attr/textAppearanceLarge" />
    
        <TextView
        android:id="@+id/NazwaY1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="40dp"
        android:text="@string/NazwaX"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/NazwaY2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="100dp"
        android:text="@string/NazwaY"
        android:textAppearance="?android:attr/textAppearanceLarge" />

   		<TextView
   		    android:id="@+id/textView1"
   		    android:layout_width="wrap_content"
   		    android:layout_height="wrap_content"
   		    android:layout_marginLeft="10dp"
   		    android:layout_marginTop="20dp"
   		    android:text="Przyspieszenie X"
   		    android:textAppearance="?android:attr/textAppearanceLarge"
   		    android:textSize="2.1mm" />
    
        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="20dp"
            android:text="Przyspieszenie Y"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="2.1mm" />
        
        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="80dp"
            android:text="Prędkość X"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="2.1mm" />
        
        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="80dp"
            android:text="Prędkość Y"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="2.1mm" />
        
         <EditText
        android:id="@+id/Adres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_marginBottom="178dp"
        android:layout_marginLeft="38dp"
        android:ems="10"
        android:hint="Tutaj podaj IP" />

    <EditText
        android:id="@+id/Port"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Adres"
        android:layout_alignTop="@+id/Adres"
        android:layout_marginTop="41dp"
        android:ems="10"
        android:hint="Tutaj podaj port" />

    <Button
        android:id="@+id/Polacz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Port"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dp"
        android:text="Połącz" />

    <Button
        android:id="@+id/Reset"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Polacz"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="18dp"
        android:text="Kalibruj" />


        
        

</RelativeLayout>

